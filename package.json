{
  "name": "split-human-name",
  "version": "2.0.2",
  "description": "Split a person's name into firstName and lastName fields",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/compwright/split-human-name.git"
  },
  "keywords": [
    "humanparse",
    "human",
    "name",
    "split",
    "first-name",
    "last-name"
  ],
  "author": "Jonathon Hill <jhill9693@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/compwright/split-human-name/issues"
  },
  "homepage": "https://github.com/compwright/split-human-name#readme",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@compwright/namecase": "^2.0.6",
    "humanparser": "^2.6.0"
  },
  "devDependencies": {
    "mocha": "^8.2.1",
    "parcel-bundler": "^1.12.4",
    "semistandard": "^16.0.0"
  },
  "main": "src/",
  "directories": {
    "test": "test"
  },
  "files": [
    "src",
    "dist/split-human-name.*"
  ],
  "scripts": {
    "lint": "semistandard --fix",
    "pretest": "npm run lint",
    "test": "mocha test",
    "prebuild": "npm test",
    "build": "parcel build src/index.js --out-file split-human-name.min.js --target browser --no-cache --global splitHumanName"
  },
  "semistandard": {
    "ignore": [
      "dist"
    ]
  }
}
